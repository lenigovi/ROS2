<robot 
    name="roboticarm_assembly1">
  <!-- Base -->
    <link 
        name="Base">
        <inertial>
        <origin
        xyz = "0.046785 -4.544 8.6164"
        rpy = "0 0 0" />
        </inertial>
        <visual>
            <origin
            xyz="0 0 0"
            rpy="0 0 0" />
            <geometry>
                <mesh 
                    filename="../meshes/AS1_P1.stl"/>
            </geometry>
            </visual>
            <collision>
                <origin
                    xyz="0 0 0"
                    rpy="0 0 0" />
            <geometry>
                <mesh 
                    filename="../meshes/AS1_P1.stl"/>
            </geometry>
            </collision>
        </link>

    <link 
        name="Link_1">
        <inertial>
        <origin
        xyz = "-0.262156 0.330897 70.072"
        rpy = "0 0 0" />
        </inertial>
        <visual>
            <origin
                xyz="0 0 0"
                rpy="0 0 0" />
            <geometry>
                <mesh 
                    filename="../meshes/AS1_P2.stl"/>
            </geometry>
            </visual>
            <collision>
            <origin
                xyz="0 0 0"
                rpy="0 0 0" />
            <geometry>
                <mesh 
                    filename="../meshes/AS1_P2.stl"/>
            </geometry>
            </collision>
    </link>


    <joint 
        name="Joint_1" type="revolute">
            <parent link="Base"/>
            <child link="Link_1"/>
            <origin xyz="0 0 0.1" rpy="0 0 0"/>
            <axis xyz="0 0 1"/>
            <limit lower="-1.57" upper="1.57" effort="10" velocity="1"/>
    </joint>



  <!-- Body -->

    <link 
        name="Body">
        <visual>
            <geometry>
            <origin
                xyz="0 0 0"
                rpy="0 0 0" />
                <mesh 
                    filename="../meshes/AS1_P10.stl"/>
            </geometry>
            </visual>
            <collision>
            <origin
                xyz="0 0 0"
                rpy="0 0 0" />
            <geometry>
                <mesh 
                    filename="../meshes/AS1_P10.stl"/>
            </geometry>
            </collision>
        </link>

    <joint 
        name="Joint_2" type="revolute">
            <parent link="Link_1"/>
            <child link="Body"/>
            <origin xyz="0 0 0.1" rpy="0 0 0"/>
            <axis xyz="0 0 1"/>
            <limit lower="-1.57" upper="1.57" effort="10" velocity="1"/>
    </joint>


    <link 
        name="Body_Upper">
        <visual>
            <origin
                    xyz="0 0 0"
                    rpy="0 0 0" />
            <geometry>
                <mesh 
                    filename="../meshes/AS1_P3.stl"/>
            </geometry>
            </visual>
            <collision>
            <origin
                xyz="0 0 0"
                rpy="0 0 0" />
            <geometry>
                <mesh 
                    filename="../meshes/AS1_P3.stl"/>
            </geometry>
            </collision>
        </link>

    <joint 
        name="Joint_3" type="revolute">
            <parent link="Body"/>
            <child link="Body_Upper"/>
            <origin xyz="0 0 0.1" rpy="0 0 0"/>
            <axis xyz="0 0 1"/>
            <limit lower="-1.57" upper="1.57" effort="10" velocity="1"/>
    </joint>


    <link 
        name="Body_Upper_Link">
        <visual>
            <geometry>
                <mesh 
                    filename="../meshes/AS1_P4.stl"/>
            </geometry>
            </visual>
            <collision>
            <geometry>
                <mesh 
                    filename="../meshes/AS1_P4.stl"/>
            </geometry>
            </collision>
        </link>


    <link 
        name="Body_End">
        <visual>
            <geometry>
                <mesh 
                    filename="../meshes/AS1_P6.stl"/>
            </geometry>
            </visual>
            <collision>
            <geometry>
                <mesh 
                    filename="../meshes/AS1_P6.stl"/>
            </geometry>
            </collision>
        </link>


    <joint 
        name="Joint_4" type="revolute">
            <parent link="Body_Upper"/>
            <child link="Body_End"/>
            <origin xyz="0 0 0.1" rpy="0 0 0"/>
            <axis xyz="0 0 1"/>
            <limit lower="-1.57" upper="1.57" effort="10" velocity="1"/>
    </joint>


</robot>